  "description": "Embed Robotframework HTML result into release and build tabs",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build:cidev": "npm run clean && webpack --mode development",
    "build": "npm run clean && webpack --mode development",
    "postbuild": "npm run package-extension",
    "postinstall": "cd buildAndReleaseTask && npm install",
    "test": "echo No test available",
    "prune": "npm prune --production --ignore-scripts",
    "package-extension": "tfx extension create --manifest-globs vss-extension.json"
  },
